{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "colored": {
          "()": "colorlog.ColoredFormatter",
          "format": "%(log_color)s%(levelname)s | %(filename)s: %(message)s",
          "log_colors": {
              "DEBUG": "yellow",
              "INFO": "green",
              "WARNING": "purple",
              "ERROR": "red",
              "CRITICAL": "white,bg_red"
          }
      }
    },
    "handlers": {
      "stdout": {
        "class": "logging.StreamHandler",
        "formatter": "colored",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "root": {
        "level": "INFO",
        "handlers": [
          "stdout"
        ]
      }
    }
  }
